{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-object-literal-type-assertion": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "quotemark": {
      "options": "single"
    },
    "no-string-literal": false,
    "max-line-length": {
      "options": [
        140
      ]
    }
  },
  "rulesDirectory": []
}